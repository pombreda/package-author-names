{
  "name": "package-author-names",
  "version": "1.0.0",
  "description": "Extract author names from package data",
  "repository": "shinnn/package-author-names",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint *.js & jscs *.js",
    "test": "bower install && node test.js",
    "coverage": "istanbul cover -x=bower_components/** test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/package-author-names/blob/master/LICENSE"
    }
  ],
  "main": "package-author-names-cjs.js",
  "files": [
    "package-author-names-cjs.js",
    "LICENSE"
  ],
  "keywords": [
    "name",
    "author",
    "authors",
    "pkg",
    "package",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "parse-author-name": "^1.0.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "eslint": "^0.9.2",
    "istanbul": "^0.3.2",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.7.3",
    "require-bower-files": "^1.0.0",
    "tape": "^3.0.1"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98
  }
}
